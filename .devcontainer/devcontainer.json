{
  "name": "stonkstats",
  "dockerComposeFile": "docker-compose.yaml",
  "service": "app",
  "remoteUser": "vscode",
  "workspaceFolder": "/workspace",
  "forwardPorts": [8050],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-python.python",
        "ms-python.flake8",
        "njpwerner.autodocstring"
      ]
    }
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "postStartCommand": "python3 debug.py"
}